{
  "name": "example",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "android": "react-native run-android",
    "compile": "tsc",
    "deps": "rnx-align-deps",
    "deps:fix": "rnx-align-deps --write",
    "ios": "react-native run-ios",
    "start": "react-native rnx-start",
    "start:reset": "yarn start --reset-cache",
    "web:build": "react-native bundle --config metro.config.web.cjs --dev false --entry-file index.js --bundle-output dist/index.bundle.js",
    "web:start": "yarn serve"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "packageManager": "yarn@3.6.3",
  "dependencies": {
    "dedent": "^1.6.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-is": "19.0.0",
    "react-native": "0.79.2",
    "react-native-spotlight-tour": "workspace:^",
    "react-native-svg": "^15.12.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.20.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@babel/runtime": "^7.27.3",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-native/babel-preset": "^0.79.2",
    "@react-native/metro-config": "^0.79.2",
    "@rnx-kit/align-deps": "^3.0.6",
    "@rnx-kit/babel-preset-metro-react-native": "^2.0.0",
    "@rnx-kit/cli": "^0.18.9",
    "@rnx-kit/metro-config": "^2.1.0",
    "@rnx-kit/metro-resolver-symlinks": "^0.2.4",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@types/react": "^19.1.6",
    "metro-config": "^0.82.4",
    "serve": "^14.2.4",
    "typescript": "^5.8.3"
  },
  "rnx-kit": {
    "kitType": "app",
    "bundle": {
      "treeShake": true,
      "plugins": [
        "@rnx-kit/metro-plugin-cyclic-dependencies-detector"
      ]
    }
  }
}
