{
  "preset": "react-native",
  "transform": {
    "^.+\\.jsx$": "babel-jest",
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "test/.*\\.test\\.(tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "json", "node"],
  "setupFilesAfterEnv": [
    "<rootDir>/test/setup.ts"
  ],
  "transformIgnorePatterns": [
    "node_modules/(!react-native)",
    "node_modules/(!@react-native-community)"
  ]
}
